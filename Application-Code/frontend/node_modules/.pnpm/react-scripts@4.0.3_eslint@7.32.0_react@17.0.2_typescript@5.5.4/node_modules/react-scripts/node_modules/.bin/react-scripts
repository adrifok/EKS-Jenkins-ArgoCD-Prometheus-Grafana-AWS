#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/adri/Documents/EKS-Jenkins-ArgoCD-Prometheus-Grafana-AWS/Application-Code/frontend/node_modules/.pnpm/react-scripts@4.0.3_eslint@7.32.0_react@17.0.2_typescript@5.5.4/node_modules/react-scripts/bin/node_modules:/home/adri/Documents/EKS-Jenkins-ArgoCD-Prometheus-Grafana-AWS/Application-Code/frontend/node_modules/.pnpm/react-scripts@4.0.3_eslint@7.32.0_react@17.0.2_typescript@5.5.4/node_modules/react-scripts/node_modules:/home/adri/Documents/EKS-Jenkins-ArgoCD-Prometheus-Grafana-AWS/Application-Code/frontend/node_modules/.pnpm/react-scripts@4.0.3_eslint@7.32.0_react@17.0.2_typescript@5.5.4/node_modules:/home/adri/Documents/EKS-Jenkins-ArgoCD-Prometheus-Grafana-AWS/Application-Code/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/adri/Documents/EKS-Jenkins-ArgoCD-Prometheus-Grafana-AWS/Application-Code/frontend/node_modules/.pnpm/react-scripts@4.0.3_eslint@7.32.0_react@17.0.2_typescript@5.5.4/node_modules/react-scripts/bin/node_modules:/home/adri/Documents/EKS-Jenkins-ArgoCD-Prometheus-Grafana-AWS/Application-Code/frontend/node_modules/.pnpm/react-scripts@4.0.3_eslint@7.32.0_react@17.0.2_typescript@5.5.4/node_modules/react-scripts/node_modules:/home/adri/Documents/EKS-Jenkins-ArgoCD-Prometheus-Grafana-AWS/Application-Code/frontend/node_modules/.pnpm/react-scripts@4.0.3_eslint@7.32.0_react@17.0.2_typescript@5.5.4/node_modules:/home/adri/Documents/EKS-Jenkins-ArgoCD-Prometheus-Grafana-AWS/Application-Code/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/react-scripts.js" "$@"
else
  exec node  "$basedir/../../bin/react-scripts.js" "$@"
fi
